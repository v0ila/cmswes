            <img class="uk-border-rounded" width="200" height="200" alt="<?php echo $this->escape(($this->getAttribute($user, 'username')) ) ?>" data-avatar="<?php echo $this->escape(($this->getAttribute($user, 'email')) ) ?>">
        </a>
    </li>
    <?php endforeach ?>
</ul>